// Online C compiler to run C program online
#include <stdio.h>
float num;
float res,cubo(int x);

int main() {
    printf("ingrese un numero");
    scanf("%f",&num);
    res=cubo(num);
    printf("%f al cubo es: %f",num,res);
    return 0;
    
}
float cubo(int x)
{return x*x*x;}

Condiciones
#include <stdio.h>
#include <stdlib.h>
int main (){
	int edad =75;
	if (edad>=18 && edad<=65){
		printf("eres mayor de edad");
	}else if (edad>65){
		printf("est√°s en edad de retiro");
	}else{
		printf("Eres menor de edad");
	}
	printf("/n");
	//system("puase");
}

#include <stdio.h>
#include <stdlib.h>
int main (){
	int opcion=1;
	switch(opcion){
	case 1:
		printf("imprimier esrado de estudiantes");
		break;
	case 2:
		printf ("solicitando lista de profesores");
		break;
	case 3:
		printf("saliendo del sistema");
		break;
	default:
		printf("opcion invalida");
	}
}

Otro ejercicio de if
#include <stdio.h>
#include <stdlib.h>
int main (){
	int x;
	printf ("introduce un numero entre 1 y 4");
	scanf ("%i", &x);
	
	if (x==1){
		printf("\nlunes.\n");
	}
	if (x==2){
		printf("martes/n");
	}
	if (x==3){
		printf("miercoles/n");
	}
	if (x==4){
		printf("jueves/n");
	}
	system("pause");
	return 0;
}


If anidados
#include <stdio.h>
#include <stdlib.h>
int main (){
	int x;
	printf ("introduce un numero entre 1 y 4");
	scanf ("%i", &x);
	
	if (x==1){
		printf("\nlunes.\n");
	}
	else if(x==2){
	printf("Martes\n");
	
	}
	else if (x==3){
		printf("miercoles\n");
	}
	else if (x==4){
		printf("jueves\n");
	}
	else{
		printf("opcion invalida");
	}
	system("pause");
	return 0;
}
	
