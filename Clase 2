// Online C compiler to run C program online
#include <stdio.h>
float num;
float res,cubo(int x);

int main() {
    printf("ingrese un numero");
    scanf("%f,&num");
    res=cubo(num);
    printf("%f al cubo es: %f",num,res);
    return 0;
    
}
float cubo(int x)
{return x*x*x;}

